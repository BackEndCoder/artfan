@charset "utf-8";
/* CSS Document */

/*Product list page css goes here*/
.pagination{
	border-bottom:1px solid #6b6b6b;
	margin:0 10px;
}
.pagination h1{
	color:#e3007e;
}
.pagination h1 span{
	font:normal 18px Helvetica, Arial, sans-serif;
	color:#868686;
}
.sort form select{
	border:1px solid #c2c2c2;
	font:normal 13px/18px 'HelveticaNeueRegular', Helvetica, Arial, sans-serif;
	padding:3px 0 3px 10px;
	width:234px;
}
