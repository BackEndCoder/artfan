@charset "utf-8";
/* CSS Document */
@font-face {
    font-family: 'HelveticaNeueRegular';
    src: url('helveticaneue.eot');
    src: url('helveticaneue.eot') format('embedded-opentype'),
         url('helveticaneue.woff') format('woff'),
         url('helveticaneue.ttf') format('truetype'),
         url('helveticaneue.svg#HelveticaNeueRegular') format('svg');
}
h1,h2,h3,h4,h5,h6{
	 font-family: 'HelveticaNeueRegular', Helvetica, Arial, sans-serif;
}

a{
	text-decoration:none;
}
.wrapper{
	width:1030px;
	margin:0 auto;
}
header{
	width:1030px;
	float:left;
	margin:10px 0px;
}
.logo{
	float:left;
	margin:0 10px;
	width:240px;
	
}
.logo h1 a{
	text-indent:-9999px;
	display:block;
	background:url(../images/artfan-logo.png) no-repeat;
	height:78px;
	width:173px;
}
.header-right{
	width:750px;
	float:left;
	margin:0 10px;
}
ul.header-cart{
	float:right;
}
ul.header-cart li{
	float:left;
}

ul.header-cart li a{
	font:bold 12px/16px 'HelveticaNeueRegular', Helvetica, Arial, sans-serif;
	color:#929497;
	padding:0 3px;
	
}
.phone{
	float:right;
	margin:40px 0 0 0;
	color:#606060;
	font:normal 20px/28px 'HelveticaNeueRegular', Helvetica, Arial, sans-serif;
}
nav{
	float:left;
	width:1010px;
	margin:0 10px;
	border:1px solid #D0D0C5;
	-webkit-border-radius:5px;
	-moz-border-radius:5px;
	border-radius:5px;
	background:url(../images/nav-bg.gif) repeat-x;
		-webkit-box-shadow: 0 8px 10px -6px #a2a2a2;
	   -moz-box-shadow: 0 8px 10px -6px #a2a2a2;
	        box-shadow: 0 8px 10px -6px #a2a2a2;

}
nav ul.menu{
	
	float:left;
	width:750px;

		-webkit-border-radius:5px;
	-moz-border-radius:5px;
	border-radius:5px;
	border-top:1px solid #E6E5D8;
		
}
nav ul.menu li{
	float:left;
	border-left:1px solid #CCCBB9;
	border-right:1px solid #A6A798;
}
nav ul.menu li a{
	font:normal 13px/26px 'HelveticaNeueRegular', Arial, sans-serif;
	padding:0 20px;
	color:#767676;

}
.signup{
	float:right;
}
.signup ul li{
	float:left
}
.signup ul li a {
		font:normal 13px/26px 'HelveticaNeueRegular', Arial, sans-serif;
	padding:0 10px;
	color:#767676;
}
.signup ul li a img{
	padding:0 3px 0 0
}
.container{
	width:1030px;
	float:left;
	margin-top:20px;
}
aside{
	float:left;
	width:240px;
	margin:0 10px;
}
.aside-box{
	float:left;
	width:240px;
	
}
.aside-box-title{
	background:#e3007e;
	margin:0 0 3px 0;
	
}
.aside-box-title h2{
	background:#a3a289;
	font-size:20px;
	color:#fff;
	margin:0 0 0 6px;
	border-left:3px solid #fff;
	padding:0 0 0 10px;
	line-height:30px;
}
.aside-contain{
	background:#dfded4;
}
.main{
	float:left;
	width:770px;
}
ul.accordion{
	margin:3px;
}
ul.accordion li{
	background:#fff;
	margin:0 0 3px 0;
}


.clr{
	clear:both;
}