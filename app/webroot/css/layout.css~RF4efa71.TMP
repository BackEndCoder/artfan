@charset "utf-8";
/* CSS Document */
@font-face {
    font-family: 'HelveticaNeueRegular';
    src: url('helveticaneue.eot');
    src: url('helveticaneue.eot') format('embedded-opentype'),
         url('helveticaneue.woff') format('woff'),
         url('helveticaneue.ttf') format('truetype'),
         url('helveticaneue.svg#HelveticaNeueRegular') format('svg');
}
h1,h2,h3,h4,h5,h6{
	 font-family: 'HelveticaNeueRegular';
}

a{
	text-decoration:none;
}
.wrapper{
	width:1030px;
	margin:0 auto;
}
header{
	width:1030px;
	float:left;
	margin:10px 0px;
}
.logo{
	float:left;
	margin:0 10px;
	width:240px;
	
}
.logo h1 a{
	text-indent:-9999px;
	display:block;
	background:url(../images/artfan-logo.png) no-repeat;
	height:78px;
	width:173px;
}
.header-right{
	width:750px;
	float:left;
	margin:0 10px;
}
ul.header-cart{
	float:right;
}
ul.header-cart li{
	float:left;
}

ul.header-cart li a{
	font:bold 12px/16px 'HelveticaNeueRegular', Helvetica, Arial, sans-serif;
	color:#929497;
	padding:0 3px;
	
}
.phone{
	float:right;
	margin:40px 0 0 0;
	color:#606060;
	font:normal 20px/28px 'HelveticaNeueRegular', Helvetica, Arial, sans-serif;
}
nav{
	float:left;
	width:1010px;
	margin:0 10px;

}
nav ul{
	background:url(../images/nav-bg.gif) repeat-x;
}
nav ul li{
	float:left;
}
.clr{
	clear:both;
}